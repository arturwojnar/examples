generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://test:test@localhost:5432/test?schema=public"
}

model TimeSlot {
  id          Int      @id @default(autoincrement())
  requesterId String
  resourceId  Int
  startTime   DateTime
  endTime     DateTime

  @@unique([resourceId, startTime], name: "resourceId_startTime_idx")
  @@index([resourceId])
}

model TimeSlot2 {
  id          Int      @id @default(autoincrement())
  requesterId String
  resourceId  Int
  startTime   DateTime
  endTime     DateTime

  @@index([resourceId])
}

/// This table contains exclusion constraints and requires additional setup for migrations. Visit https://pris.ly/d/exclusion-constraints for more info.
model timeslot3 {
  id          Int                       @id @default(autoincrement())
  requesterid String?
  resourceid  Int?
  date_range  Unsupported("daterange")?
}
