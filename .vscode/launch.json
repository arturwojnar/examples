{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Counter",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "postgresql-counter/dist/index.js",
      "runtimeArgs": ["--experimental-modules"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Availability",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "postgresql-availability/dist/index.js",
      "runtimeArgs": ["--experimental-modules"]
    },
    {
      "args": [
        "-c",
        "${workspaceFolder}/postgresql-availability/jest.config.js",
        "--maxConcurrency=1",
        "--runInBand",
        "--detectOpenHandles",
        "--colors",
        "--verbose"
      ],
      "internalConsoleOptions": "neverOpen",
      "console": "integratedTerminal",
      "name": "test",
      "program": "${workspaceRoot}/postgresql-availability/node_modules/.bin/jest",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    }
  ]
}